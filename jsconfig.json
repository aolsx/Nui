{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "paths":{
      "@/*":["./src/*"]
    }
  },
  "exclude": [ "node_modules", "build" ],
}